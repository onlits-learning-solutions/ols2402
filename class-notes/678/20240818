
	class Switch {
		bool mode = false;
		
		void on() {
			mode = true;
		}
		
		void off() {
			mode = false;
		}
		
		int getMode() {
			return mode;
		}
	}
	
	interface	-	not available in C++
	
	boolean	-	binary
	
	
	if(n == 10)	-	0 or non-zero
	
	if(n==0)	-	TRUE or FALSE
	
